# OMNIX AI Environment Variables

# API Configuration
VITE_API_BASE_URL=https://api.omnix-ai.com
VITE_API_TIMEOUT=10000
VITE_WEBSOCKET_URL=wss://api.omnix-ai.com/ws

# CDN Configuration
VITE_CDN_URL=https://cdn.omnix-ai.com
VITE_CDN_IMAGES=https://images.omnix-ai.com
VITE_CDN_STATIC=https://static.omnix-ai.com
VITE_CDN_FONTS=https://fonts.omnix-ai.com

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Error Tracking
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PWA=true
VITE_ENABLE_CDN=true

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLE_RATE=0.1

# Security
VITE_CSP_REPORT_URI=/api/csp-report
VITE_ENABLE_SECURITY_HEADERS=true

# SEO
VITE_SITE_NAME=OMNIX AI
VITE_SITE_DESCRIPTION=Smart inventory management system with AI-powered insights
VITE_SITE_URL=https://omnix-ai.com
VITE_SITE_IMAGE=https://omnix-ai.com/og-image.jpg

# Social Media
VITE_TWITTER_HANDLE=@omnixai
VITE_FACEBOOK_APP_ID=your_facebook_app_id